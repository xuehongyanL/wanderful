{
  "name": "wanderful",
  "version": "0.2.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "dev": "parcel index.html -p 2333",
    "build": "parcel build index.html --no-content-hash --out-dir docs --public-url ."
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@mapbox/geojson-rewind": "^0.4.0",
    "@mapbox/geojsonhint": "^3.0.0",
    "bootstrap": "^4.3.1",
    "file-saver": "^2.0.2",
    "geojson": "^0.5.0",
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4",
    "lodash": "^4.17.15",
    "react": "^16.3.0",
    "react-ace": "^7.0.1",
    "react-dom": "^16.3.0",
    "react-leaflet": "^2.3.0",
    "react-leaflet-draw": "^0.19.0",
    "reactstrap": "^8.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jsdom-worker": "^0.1.0",
    "node-fetch": "^2.6.0",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.20.3"
  }
}
